{
  "name": "perfect-derm.com",
  "description": "Nuxt.js + Netlify CMS project",
  "author": "webo <support@webo.agency>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "dev-debug": "cross-env NODE_ENV=debug node --inspect-brk node_modules/.bin/nuxt",
    "build": "nuxt build",
    "analyze": "nuxt build --analyze",
    "start": "nuxt build && nuxt start",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint-fix": "eslint --ext .js,.vue --ignore-path .gitignore . --fix",
    "precommit": "npm run lint",
    "deploy-test": "cross-env NODE_ENV=production node --inspect-brk node_modules/.bin/nuxt generate",
    "deploy": "nuxt generate"
  },
  "dependencies": {
