    "@nuxtjs/google-tag-manager": "^2.3.2",
    "@nuxtjs/markdownit": "^1.2.9",
    "@nuxtjs/pwa": "^3.0.0-beta.20",
    "@nuxtjs/robots": "^2.4.2",
    "@nuxtjs/sitemap": "^2.0.1",
    "@nuxtjs/tailwindcss": "^1.4.0",
    "aos": "^2.3.4",
    "core-js": "^2.6.11",
    "cross-env": "^7.0.2",
    "glob": "^7.1.6",
    "imagemin": "^7.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^8.0.0",
